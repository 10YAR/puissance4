{
    "name": "puissance4",
    "private": true,
    "version": "0.0.0",
    "engines": {
        "node": "16.x"
    },
    "scripts": {
        "dev": "concurrently \"npm:start:client\" \"npm:start:server\" ",
        "start:server": "nodemon src/server/index.ts",
        "start:client": "vite",
        "start": "npx esmo src/server/server.ts",
        "build": "esbuild src/server/*.ts --platform=node --outdir=build --resolve-extensions=.js",
        "test": "vitest",
        "test:watch": "vitest watch"
    },
    "dependencies": {
        "@fastify/static": "5.0.0",
        "@fastify/websocket": "5.0.0",
        "fastify": "^3.25.3",
        "fastify-socket.io": "^4.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.3.0",
        "reconnecting-websocket": "^4.4.0",
        "socket.io-client": "^4.5.2",
        "tailwindcss": "^3.1.8",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.0.0",
        "@types/node": "^18.7.15",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@types/tailwindcss": "^3.1.0",
        "@types/uuid": "^8.3.4",
        "@types/ws": "^8.5.3",
        "@vitejs/plugin-react": "^2.0.1",
        "@vitest/coverage-c8": "^0.23.1",
        "autoprefixer": "^10.4.8",
        "concurrently": "^7.3.0",
        "esbuild": "^0.15.7",
        "esmo": "^0.14.0",
        "nodemon": "^2.0.19",
        "postcss": "^8.4.16",
        "ts-node": "^10.9.1",
        "types-fastify-socket.io": "^0.0.1",
        "typescript": "^4.6.4",
        "vite": "^3.0.7",
        "vitest": "^0.23.1"
    },
    "nodemonConfig": {
        "exec": "esmo",
        "ext": "js,json,ts,tsx"
    }
}
